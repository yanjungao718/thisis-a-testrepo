					" path {\r\n",
					" fill: white;\r\n",
					" stroke: #000;\r\n",
					" }\r\n",
					" circle {\r\n",
					" fill: #fff;\r\n",
					" stroke: #000;\r\n",
					" pointer-events: none;\r\n",
					" }\r\n",
					" .PiYG .q0-9{fill:rgb${colors(0)}}\r\n",
					" .PiYG .q1-9{fill:rgb${colors(1)}}\r\n",
					" .PiYG .q2-9{fill:rgb${colors(2)}}\r\n",
					" .PiYG .q3-9{fill:rgb${colors(3)}}\r\n",
					" .PiYG .q4-9{fill:rgb${colors(4)}}\r\n",
